doctype html
html
    head
        title= title
        meta(charset="utf-8")
        meta(name="description", content="HacKSU is the computer science student organization at Kent State University.")
        meta(name="author", content="")
        meta(name="viewport", content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no")
        link(rel='icon', href='/images/favicon-32x32.png', type='image/png')
        link(rel="stylesheet", href="/stylesheets/main.css", media="screen")
        link(rel="stylesheet", href="/stylesheets/accordian.css", media="screen")
        link(rel="stylesheet", href="/stylesheets/bootstrap.min.css", media="screen")
        link(rel="stylesheet", href="/stylesheets/bootstrap-theme.min.css", media="screen")
        link(href="https://fonts.googleapis.com/icon?family=Material+Icons",rel="stylesheet")
        link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600', rel='stylesheet', type='text/css')
        script(src="https://npmcdn.com/react@15.3.0/dist/react-with-addons.js")
        script(src="https://npmcdn.com/react-dom@15.3.0/dist/react-dom.js")
        block extra-head

    body
        // Wrap all page content here
        div#wrap
            div.navbar.navbar-default
                div.navbar-header
                    button.navbar-toggle(type="button", data-toggle="collapse", data-target=".navbar-collapse")
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.navbar-brand(href="/")
                      img(src="/images/hacksu.png")
                div.collapse.navbar-collapse
                    ul.nav.navbar-nav
                        li#nextup
                        li
                            a(href="https://khe.io") Kent Hack Enough
                        li
                            a(href="/contact") Contact
                        li
                            a.social-item(href="https://www.facebook.com/groups/hacksu/")
                              img(src="/images/facebook.png")
                        li
                            a.social-item(href="https://twitter.com/_hacksu")
                              img(src="/images/twitter.png")
                        li
                            a.social-item(href="https://github.com/hacKSU")
                              img(src="/images/github.png")
            block content
        div#footer
            div.container
                ul.list-inline.sub-nav
                    li
                        a(href="/#about-info", onclick="showAbout();return false;") About
                    li
                        a(href="https://khe.io") Kent Hack Enough
                    li
                        a(href="/contact") Contact
                p.text-muted.credit &copy; 2015 HacKSU
        script(src="/javascripts/jquery-2.0.3.min.js")
        script(src="/dist/bundle.js")
        script(src="/javascripts/bootstrap.min.js")
        script(src="/javascripts/main.js")
        script(src="/javascripts/retina-1.1.0.min.js")
        script.
            var _gaq = [
                ['_setAccount', 'UA-35084827-1'],
                ['_trackPageview']
            ];
            (function(d, t) {
                var g = d.createElement(t),
                    s = d.getElementsByTagName(t)[0];
                g.src = ('https:' == location.protocol ? '//ssl' : '//www') + '.google-analytics.com/ga.js';
                s.parentNode.insertBefore(g, s)
            }(document, 'script'));
